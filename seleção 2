package main

//Faça um algoritmo que leia três números inteiros e mostre o menor deles.
import "fmt"

func main() {
	var n1 int
	var n2 int
	var n3 int
	fmt.Print("Escolha um número ")
	fmt.Scan(&n1)
	fmt.Print("Escolha outro número ")
	fmt.Scan(&n2)
	fmt.Print("Escolha um terceiro número ")
	fmt.Scan(&n3)
	if n1 < n2 && n1 < n3 {
		fmt.Println("O primeiro número é menor")
	} else if n1 == n2 && n2 == n3 {
		fmt.Println("Os números são iguais")
	} else if n1 > n2 && n3 > n2 {
		fmt.Println("O segundo número é menor")
	} else {
		fmt.Println("O terceiro número é menor")
	}
}
