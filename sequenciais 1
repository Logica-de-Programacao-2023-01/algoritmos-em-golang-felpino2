package main

import "fmt"

func main() {
	var n1 float64
	var n2 float64
	var n3 float64
	fmt.Print("Qual o primeiro número? ")
	fmt.Scan(&n1)
	fmt.Print("Qual o segundo número? ")
	fmt.Scan(&n2)
	fmt.Print("Qual o terceiro número? ")
	fmt.Scan(&n3)
	fmt.Println("A soma desses números é ", n1+n2+n3)
}
